  "scripts": {
    "start": "egg-scripts start --daemon --title=price",
    "stop": "egg-scripts stop --title=price",
    "dev": "egg-bin dev",
    "test": "egg-bin test",
    "cov": "egg-bin cov",
    "dev-web": "cross-env PUBLIC_PATH=/price/ webpack-dev-server --progress --open --hot --config webpack.dev.js",
    "build": "cross-env PUBLIC_PATH=/price/ webpack --progress --config webpack.prod.js",
    "build-dll": "webpack --progress --config webpack.dll.js",
    "build-analyze": "cross-env analyze=true npm run build"
  },
  "author": "pikou",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "echo $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.eslintignore --write",
      "eslint --fix --ext .js,.jsx,.ts,.tsx",
      "git add"
    ]
  }
